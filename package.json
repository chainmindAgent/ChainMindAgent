{
    "name": "chainmind",
    "version": "1.0.0",
    "description": "ChainMind - BNBChain Knowledge AI Agent by OpenClaw",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "train": "tsx src/cli.ts train",
        "web": "tsx src/web/server.ts",
        "build:ui": "cd src/web/ui && npx vite build",
        "post:moltbook": "tsx src/cli.ts post-moltbook",
        "post:twitter": "tsx src/cli.ts post-twitter",
        "test": "vitest",
        "test:integration": "vitest run --config vitest.integration.config.ts",
        "cli": "tsx src/cli.ts"
    },
    "keywords": [
        "ai",
        "agent",
        "bnbchain",
        "moltbook",
        "openclaw"
    ],
    "author": "ChainMind",
    "license": "MIT",
    "dependencies": {
        "@langchain/core": "^0.3.58",
        "@langchain/openai": "^0.3.16",
        "@steipete/bird": "^0.8.0",
        "axios": "^1.6.7",
        "cors": "^2.8.5",
        "dotenv": "^16.4.1",
        "ethers": "^6.16.0",
        "express": "^4.18.2",
        "langchain": "^0.3.5",
        "node-cron": "^3.0.3",
        "openai": "^4.28.0",
        "sql.js": "^1.10.2",
        "twitter-api-v2": "^1.15.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@nomicfoundation/hardhat-toolbox": "^5.0.0",
        "@openzeppelin/contracts": "^5.4.0",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.16",
        "@types/node-cron": "^3.0.11",
        "hardhat": "^2.22.15",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3",
        "vitest": "^1.2.2"
    }
}